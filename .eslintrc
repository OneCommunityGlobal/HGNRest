{
  "extends": ["eslint:recommended","airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": { 
    "ecmaVersion": 8,
    "sourceType": "module"
   },
  "env": { "es6": true,
     "node": true,
    "commonjs": true
    },
  "rules": {
    "global-require": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign" : "off",
    "max-len": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }

  
}
