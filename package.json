    "node-datetime": "^2.0.3",
    "nodemailer": "^6.10.0",
    "redis": "^4.2.0",
    "regression": "^2.0.1",
    "sanitize-html": "^2.16.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "supertest": "^6.3.4",
    "telesignsdk": "^3.0.4",
    "twilio": "^5.10.2",
    "uuid": "^3.4.0",
    "ws": "^8.17.1"
  },
  "nodemonConfig": {
    "watch": [
      "/src/**/*"
    ]
  }
}
