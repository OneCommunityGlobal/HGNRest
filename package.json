{
  "name": "hgnrest",
  "version": "1.0.0",
  "description": "This module is the REST module built on NodeJS to support MongoDB activities, Since EmberData has no direct adapters for MongoDB. This module will later be intergrated with EmberJS HGN project to maintain singularity.",
  "main": "restapp",
  "scripts": {
    "test": "test",
    "lint": "eslint --fix ./src",
    "build": "babel src -d dist",
    "buildw": "babel src -d dist --watch",
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "serve": "babel-node src/server.js"
  },
  "pre-commit": [
    "lint"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "lint"
  },
  "author": "AK",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/node": "^8.10.61",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.3.0",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-async-to-generator": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@sentry/node": "^5.17.0",
    "abort-controller": "^3.0.0",
    "async-exit-hook": "^2.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.4",
    "cron": "^1.8.2",
    "dotenv": "^5.0.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "googleapis": "^100.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
